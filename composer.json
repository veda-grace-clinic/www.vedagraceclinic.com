{
  "repositories": [
    {
      "type": "composer",
      "url": "https://composer.deliciousbrains.com"
    },
    {
      "type": "composer",
      "url": "https://wpackagist.org"
    }
  ],
  "extra": {
    "installer-paths": {
      "./site/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "./site/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "require": {
    "wpackagist-plugin/wp-native-php-sessions": "*",
    "wpackagist-plugin/contact-form-7": "*",
    "deliciousbrains-plugin/wp-migrate-db-pro": "*",
    "deliciousbrains-plugin/wp-migrate-db-pro-media-files": "*",
    "deliciousbrains-plugin/wp-offload-media": "*",
    "deliciousbrains-plugin/wp-offload-media-assets-pull": "*"
  }
}
